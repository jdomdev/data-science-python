    "def refact(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        print(n)\n",
    "        p = n * refact(n - 1)\n",
    "        print(p)\n",
    "        return p\n",
    "factorial = refact(int(input('Enter any number: ')))\n",
    "print('Resultado = ', factorial)    "
